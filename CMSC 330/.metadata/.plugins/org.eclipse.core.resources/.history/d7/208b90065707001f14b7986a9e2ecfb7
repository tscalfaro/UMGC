#ifndef TERNARY_H_
#define TERNARY_H_

class Ternary: public SubExpression {
public:
	Ternary(Expression* left, Expression* middle, Expression* right): SubExpression(left, right), middle(middle){
		}
		double evaluate();
};

#endif /* TERNARY_H_ */
