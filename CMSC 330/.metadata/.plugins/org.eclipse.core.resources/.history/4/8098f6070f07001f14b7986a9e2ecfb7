#ifndef NEGATE_H_
#define NEGATE_H_

class Negate: public SubExpression{
public:
	Negate(Expression* left, Expression* right): SubExpression(left, right){
	}
	double evaluate() {
	       return left->evaluate() * -1;
	    }
};



#endif /* NEGATE_H_ */
