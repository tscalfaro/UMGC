#ifndef NEGATE_H_
#define NEGATE_H_

class Negate: public SubExpression{
public:
	Negate(Expression* left): SubExpression(left, nullptr){
	}
	double evaluate()override {
	       return left->evaluate() * -1;
	    }
};



#endif /* NEGATE_H_ */
