#include <cmath>
#ifndef EXPONENT_H_
#define EXPONENT_H_

class Exponent: public SubExpression {
	Exponent(Expression* left, Expression* right): SubExpression(left, right){
		}
		double evaluate() {
		       return pow(left->evaluate(), right->evaluate());
		    }
};



#endif /* EXPONENT_H_ */
