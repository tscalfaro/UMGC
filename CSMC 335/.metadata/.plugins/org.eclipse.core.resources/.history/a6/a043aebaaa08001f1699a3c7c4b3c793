package application;

public class TrafficLight {
	private TrafficLightState state;
	
	public TrafficLight() {
		this.state = TrafficLightState.RED;
	}
	
	public TrafficLightState getTrafficLightState() {
		return state;
	}
	
	public void setTrafficLightState(TrafficLightState state) {
		this.state = state;
	}
}
