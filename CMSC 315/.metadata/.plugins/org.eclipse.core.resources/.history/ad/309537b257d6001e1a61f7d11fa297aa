package application;

public class Vertex {

	double x;
	double y;
	char name;
	
	public Vertex(double x, double y, char name) {
		this.x = x;
		this.y = y;
		this.name = name;
	}
	
	public double getX() {
		return x;
	}
	
	public double getY() {
		return y;
	}
	
	public char getName() {
		return name;
	}
}
