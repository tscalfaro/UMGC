SELECT FIRST_NAME, LAST_NAME, ACCOUNT_NUMBER, ADDRESS
FROM CUSTOMER
ORDER BY ACCOUNT_NUMBER;

SELECT *
FROM CUSTOMER_MOVIE
WHERE DATE_RENTED >= SYSDATE - 30
ORDER BY DATE_RENTED;

SELECT *
FROM DISTRIBUTOR
ORDER BY NAME;

UPDATE CUSTOMER
SET LAST_NAME = 'Doe'
WHERE CUSTOMER_ID = 2;


DELETE FROM CUSTOMER
WHERE CUSTOMER_ID = 5;

SELECT *
FROM CUSTOMER;